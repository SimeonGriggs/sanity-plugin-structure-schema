{
  "name": "sanity-plugin-structure-schema",
  "version": "0.0.1",
  "description": "Create Sanity Desk Structure with a similar syntax to Schema",
  "main": "lib/structure-schema.js",
  "scripts": {
    "lint": "eslint .",
    "build": "sanipack build",
    "watch": "sanipack build --watch",
    "prepublishOnly": "sanipack build && sanipack verify"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SimeonGriggs/sanity-plugin-structure-schema.git"
  },
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "author": "Simeon Griggs <simeon@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "@sanity/desk-tool": "^2.21.9",
    "speakingurl": "^14.0.1"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-sanity": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "prettier": "^2.4.1",
    "sanipack": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "bugs": {
    "url": "https://github.com/SimeonGriggs/sanity-plugin-structure-schema/issues"
  },
  "homepage": "https://github.com/SimeonGriggs/sanity-plugin-structure-schema#readme",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "sanipack/babel/eslint-parser",
    "extends": [
      "sanity",
      "sanity/react",
      "prettier"
    ],
    "ignorePatterns": [
      "lib/**/"
    ]
  }
}
